PGM	START	100	.Starting at Address 100
	LDA	ONE	.Initialize the list
	STA	ARRAY
	LDX	INDEX
	LDA	TWO
	STA	ARRAY,X
	LDA	INDEX
	ADDR	A,X
	LDA	THREE
	STA	ARRAY,X
	LDA	INDEX
	ADDR	A,X
	LDA	FOUR
	STA	ARRAY,X
	LDA	INDEX
	ADDR	A,X
	LDA	FIVE
	STA	ARRAY,X
	LDA	INDEX
	ADDR	A,X
	LDA	SIX
	STA	ARRAY,X
	LDA	INDEX
	ADDR	A,X
	LDA	SEVEN
	STA	ARRAY,X
	LDA	INDEX
	ADDR	A,X
	LDA 	EIGHT
	STA	ARRAY,X
	LDA	INDEX
	ADDR	A,X
	LDA	NINE
	STA	ARRAY,X
	LDA	INDEX
	ADDR	A,X
	LDA	TEN
	STA	ARRAY,X
	LDA	INDEX
	ADDR	A,X	.Ok, Now we can play
	LDA	ARLEN	.Initialize END
	SUB	#1
	STA	END
	LDA	#0	.Initialize START
	STA	START
BEGIN	LDA	INDEX	.Get Value at START Index
	MUL	START
	RMO	A,X
	LDT	ARRAY,X	.Load Value into Register T
	LDA	INDEX	.Store Value into END Index
	MUL	END
	RMO	A,X
	RMO	T,A
	STA	REVARY,X
	LDA	START	.Increment Start
	ADD	#1
	STA 	START
	LDA	END	.Decrement End
	SUB	#1
	STA	END
	LDA	ARLEN
	COMP	START	.Compare START to ARLEN
	JGT	BEGIN
	RSUB
ONE	WORD	12	.Starting Initial List of Integers
TWO	WORD	54
THREE	WORD	81
FOUR	WORD	62
FIVE	WORD	27
SIX	WORD	94
SEVEN	WORD	19
EIGHT	WORD	20
NINE	WORD	19
TEN	WORD	65
ARLEN	WORD	10	.Length of the Array
INDEX	WORD	3	.As a constant
ARRAY 	RESW	10	.Initial Array
REVARY	RESW	10	.Reversed Array
START	RESW	1	.The Start
END	RESW	1	.End of the Array
	END	PGM
